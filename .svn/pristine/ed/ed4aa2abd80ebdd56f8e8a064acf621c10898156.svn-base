package localisation.converters;

import javax.swing.ImageIcon;

import localisation.StringConverter;



/**
 * Konvertor {@code String} objekta u {@link javax.swing.ImageIcon} objekat. String je u formatu relativne putanje slike.
 * 
 * @author danijel
 * @version 1.0, 15.11.2015.
 * @since 1.0
 * @see javax.swing.ImageIcon#ImageIcon(String)
 *
 */
public class ImageIconConverter implements StringConverter<ImageIcon> {

	@Override
	public ImageIcon convert(String s) {
		return new ImageIcon(s);
	}

}
