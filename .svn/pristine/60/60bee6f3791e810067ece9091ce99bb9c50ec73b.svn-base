package localisation;

/**
 * Klasa kojom od Stringa pravimo objekat radi njegove lakše lokalizacije
 * 
 * @author Ivana
 */

public class LocaliseName {
	String name;
	
	public LocaliseName(String n){
		this.name = n;
	}

	public void setName(String n){
		this.name = n;
	}
	
	public String getName(){
		return this.name;
	}
}
